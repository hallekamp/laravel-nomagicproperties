{
  "name": "hallekamp/laravel-nomagicproperties",
  "description": "This package make it possible to define public properties on a model without breaking default Laravel magic.",
  "type": "library",
  "require": {
    "php": "^7.0",
    "illuminate/support": ">6.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Hallekamp",
      "email": "jan@hallekamp.de"
    }
  ],
  "minimum-stability": "stable",
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "orchestra/testbench": "~3"
  },
  "autoload": {
    "psr-4": {
      "Hallekamp\\NoMagicProperties\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hallekamp\\NoMagicProperties\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hallekamp\\NoMagicProperties\\PackageServiceProvider"
      ]
    }
  }
}
